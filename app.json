{
  "expo": {
    "name": "post-ir",
    "slug": "post-ir",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-post.png",
    "scheme": "postapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "hidden": false
    },
    "ios": {
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDMxvfpF17Kv8QE4DM3kLGb8QyNgyWKS-I"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-post.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.postir.app",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-post.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow post-ir to use your location."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "useCleartextTraffic": true
          },
          "ios": {
            "flipper": true
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "27eaeb60-277f-4ffa-92c7-16524bb033df"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/27eaeb60-277f-4ffa-92c7-16524bb033df"
    }
  }
}
